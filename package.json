{
  "name": "mern_app",
  "version": "1.0.0",
  "description": "Time manager app build with MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "engines": {
    "node": "14.16.1",
    "npm": "6.14.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/N-Feriel/MERN.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/N-Feriel/MERN/issues"
  },
  "homepage": "https://github.com/N-Feriel/MERN#readme",
  "dependencies": {
    "concurrently": "^6.1.0",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "mongoose": "^5.12.9"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
